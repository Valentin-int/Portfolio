{% extends "parent/layout_admin.html.twig" %}

{% block body %}
	<h1>Project</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ project.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ project.name }}</td>
			</tr>
			<tr>
				<th>Url_project</th>
				<td>{{ project.urlProject }}</td>
			</tr>
		</tbody>
	</table>
	{{ form_start(formImage) }}
	{{ form_widget(formImage) }}
	<button class="btn create-button">{{ button_label|default('Save') }}</button>
	{{ form_end(formImage) }}

    {{ form_start(formContributor) }}
	{{ form_widget(formContributor) }}
	<button class="btn create-button">{{ button_label|default('Save') }}</button>
	{{ form_end(formContributor) }}

    {{ form_start(formTechno) }}
	{{ form_widget(formTechno) }}
	<button class="btn create-button">{{ button_label|default('Save') }}</button>
	{{ form_end(formTechno) }}

	<a href="{{ path('admin_project_index') }}">back to list</a>

	<a href="{{ path('admin_project_edit', {'id': project.id}) }}">edit</a>

	{{ include('admin/project/_delete_form.html.twig') }}
{% endblock %}
